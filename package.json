  "scripts": {
    "build": "rm -rf dist rm -rf dist-post-ts && tsc && ts-node bin/build.ts",
    "docker-compose:up": "docker compose up --detach --build --wait",
    "docker-compose:down": "docker compose down --remove-orphans",
    "generate": "graphql-codegen --config codegen.yml",
    "lint": "tsc --noEmit && eslint . --ext .ts,.js",
    "pretest": "npm run docker-compose:up",
    "posttest": "npm run docker-compose:down",
    "test": "jest --runInBand",
    "test:ci": "jest --runInBand --ci",
    "maintenance-job": "npx ts-node src/maintenance-jobs/job-runner.ts"
  },
